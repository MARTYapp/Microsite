{
  "extends": ["next/core-web-vitals"],
  "rules": {
    "semi": ["error", "never"],
    "quotes": ["error", "single"],
    "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }],
    "react/react-in-jsx-scope": "off",
    "object-curly-spacing": ["error", "always"],
    "comma-dangle": ["error", "only-multiline"]
  }
}